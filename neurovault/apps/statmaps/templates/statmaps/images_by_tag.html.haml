{% extends "base.html" %}

{% block includes %}
	:javascript
        var images = [
        {% for image in images %}
        {
            'name': '{{ image.name }}',
            'url': '{{ image.json_path }}',
            'json': true,
            'colorPalette': 'red'
        },
        {% endfor %}]
	</script>
	{% include 'statmaps/_viewer_head.html.haml' %}
{% endblock %}

{% block content %}
<h1>{{ tag }}</h1>
{% include 'statmaps/_viewer_content.html.haml' %}
{% for image in images %}
    <li><a href="{% url 'image_details' image.id %}">{{ image.name }}</a></li>
{% endfor %}

</div>
{% endblock %}